{
  "name": "chic",
  "cwd": "/home/web/chic",
  "script": "server/index.js",
  "error_file": "./log/err.log",
  "out_file": "./log/out.log",
  "log_date_format": "YYYY-MM-DD HH:mm Z",
  "pid_file": "./log/chic.pid",
  "min_uptime": "100s",
  "instances": 0,
  "exec_mode": "fork",
  "max_restarts": 10,
  "merge_logs": true,
  "max_memory_restart": "400M",
  "autorestart": true,
  "env": {
    "NODE_ENV"  : "production",
    "TEST" : 0
  }
}
